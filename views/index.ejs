<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>mini-message-board</title>
  </head>
  <body>
    <ul>
      <% messages.forEach((message) => { %>
      <div>
        <p><%= message.text %></p>
        <p><%= message.user %></p>
        <p><%= message.added %></p>
      </div>
      <% }) %>
    </ul>
  </body>
</html>
